{
  "name": "my-nextjs-auth-with-app-router",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db": "npm run db:sync && npm run db:client:generate",
    "db:sync": "dotenv -e .env.local -- npx prisma db push --force-reset",
    "db:client:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:view": "dotenv -e .env.local -- npx prisma studio"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.1",
    "@prisma/client": "^5.16.2",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "prisma-field-encryption": "^1.5.2",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^5.16.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
